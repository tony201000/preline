{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/no-unescaped-entities": 0,
    "quotes": ["error", "double"],
    "object-curly-spacing": ["error", "always"],
    "linebreak-style": ["error", "unix"]
  }
}
